{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "module": "es2020",
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "knowledge-base-lib": [
        "dist/knowledge-base-lib/knowledge-base-lib",
        "dist/knowledge-base-lib"
      ],
      "ws-soap-lib": [
        "dist/ws-soap-lib/ws-soap-lib",
        "dist/ws-soap-lib"
      ]
    }
  }
}